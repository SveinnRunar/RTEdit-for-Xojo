#tag WindowCode
	#tag Event
		Sub Opening()
		  self.Title = self.Title + " " + RTEdit.Version
		  labelAbout.Text = labelAbout.Text.Replace("<version>", App.RTEditVersion)
		  
		  
		  
		End Sub
	#tag EndEvent


	#tag Method, Flags = &h21
		Private Sub DeleteOpenAIKey()
		  var fileData as new JSONItem
		  app.OpenAIKey = ""
		  fileData.Value("OpenAIKey") = app.OpenAIKey
		  mFolderItem.CreateSettingsFileInApplicationFolder("RTEdit",fileData)
		End Sub
	#tag EndMethod

	#tag Method, Flags = &h21
		Private Sub ShowOpenAIKeyMessage()
		  var messageOpenAI As String
		  if app.OpenAIKey.Trim = "" then messageOpenAI = "(You have no OpenAI key stored)." else messageOpenAI = "(An OpenAI key is currently stored in the preference file)"
		  
		  labelDescription.Text = "Remove the stored OpenAI key from the App's preference file " + messageOpenAI
		  
		End Sub
	#tag EndMethod


	#tag Constant, Name = K_ABOUT, Type = String, Dynamic = False, Default = \"We developed RTEdit to be used for our Application\x2C ELDR. We depend heavily on the control for both AI backends\x2C as well as displaying and editing formatted documents. You can download the App for free and check out how we use the control for various tasks and features.\n\n", Scope = Private
	#tag EndConstant

	#tag Constant, Name = K_BASIC, Type = String, Dynamic = False, Default = \"RTEdit using only basic control settings. We use theme \'Bubble\' which has no visible toolbar but a floating toolbar appears when you select text. All common key shortcuts work (Control/Command B / I / U etc.) for common font styling. \n\nIn addition\x2C the control in the demo has keybindings \x3D true\x2C which means that you can use Control/Command 1 / 2 / 3 / 4 to set various heading styles.\n\nType 1. or - to create lists and bulletins\x2C correspondingly.", Scope = Private
	#tag EndConstant

	#tag Constant, Name = K_CHATBOT, Type = String, Dynamic = False, Default = \"RTEdit used to stream a remote ChatBot response\x2C such as from ChatGPT\x2C using the AppendText method.  RTEdit supports a stream of rich text data\x2C and will handle the font styling for you. \n\nDemo requires an OpenAI Key to work and you will be triggered for your OpenAI key if you have not already entered it.\n\nThis demo uses AIKit by Garry Petters to connect to ChatGPT. You can download AIKit here : http://github.com/gkjpettet/AIKit\n\n", Scope = Private
	#tag EndConstant

	#tag Constant, Name = K_IDE, Type = String, Dynamic = False, Default = \"A demonstration of RTEdit\x2C where all settings of the control are defined within the IDE itself\x2C with no code needed.", Scope = Private
	#tag EndConstant

	#tag Constant, Name = K_IMPORT_EXPORT, Type = String, Dynamic = False, Default = \"A demo showcasing how you can easily Import / Export not only the content of the editor\x2C but also it\'s variables\x2C settings\x2C color scheme\x2C tags and other attributes. With a single line you can Export or Import a document with all or some of these settings. ", Scope = Private
	#tag EndConstant

	#tag Constant, Name = K_PLACEHOLDER, Type = String, Dynamic = False, Default = \"Choose a demo to get started.", Scope = Private
	#tag EndConstant

	#tag Constant, Name = K_SANDBOX, Type = String, Dynamic = False, Default = \"RTEdit sandbox\x2C showcasing the use of all properties / methods and settings for the control.\n\nWhen adding / removing supported font styling\x2C press \'Apply styling changes\' in order for those changed to take effect.", Scope = Private
	#tag EndConstant

	#tag Constant, Name = K_TABLES, Type = String, Dynamic = False, Default = \"A demo showcasing use of Tables with RTEdit.\n\nRight click the control inside and outside of a table to see how you can create custom contextual menus depending on wether a table is selected or not. You can also create tables up to 10x10 rows and columns\x2C using source code. \n\nUse the TableLabel... and TabelTooltip... properties to set custom names for tooltips and labels in the table and cell properties modals.\n\nBrowse columns and cells using the arrow keys.\n", Scope = Private
	#tag EndConstant

	#tag Constant, Name = K_VARIABLES, Type = String, Dynamic = False, Default = \"RTEdit\'s Variables module is based on Quill-Mentions module\x2C but with so much functionality added\x2C that we renamed it to \'Variables\' to be more descriptive. \n\nThe module is enabled with RTEdit.AllowVariables\x2C which can be set by code or within the IDE. Supported characters that can be used to insert variables are : @ # $ % { [\n", Scope = Private
	#tag EndConstant


