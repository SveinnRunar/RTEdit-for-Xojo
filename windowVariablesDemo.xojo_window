	#tag Method, Flags = &h0
		Sub GetVariables()
		  var sArray() as string = RTEdit1.getDocumentVariables
		  
		  textVariables.Text = ""
		  
		  var arrayLastIndex As Integer = sArray.LastIndex
		  for i as integer = 0 to arrayLastIndex
		    textVariables.Text = textVariables.Text + sArray(i) + EndOfLine
		    
		  next
		  
		End Sub
	#tag EndMethod


