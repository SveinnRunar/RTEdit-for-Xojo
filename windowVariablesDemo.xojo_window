#tag Events RTEdit1
	#tag Event
		Sub EditorReady()
		  // Editor is ready, load a sample document which uses variables.
		  RTEdit1.Document = K_VARIABLE_DOCUMENT_SAMPLE
		End Sub
	#tag EndEvent
	#tag Event
		Sub Opening()
		  // Add some variables to our control, in our controls opening event so that they will be passed to the control
		  // BEFORE the initializing process.
		  
		  // If you pass any setting to the control After the opening event or after the EditorReady event, you need to 
		  // manually call RTEdit.Initialize to update the control.
		  
		  // You can bypass this rule by turning AutoInitialize off, RTEdit1.AutoInitialize = False either by code or in the IDE
		  // This way, the initializing process is not triggered until you specifically fire it. In that case, you can place your code 
		  // in the windows opening event, and at the end of it, fire RTEdit.Initialize, manually.
		  
		  // Set colorscheme
		  RTEdit1.ColorScheme = RTEdit.ColorSchemes.None
		  
		  // Variables
		  // -----------
		  
		  // You can assign variables to the control, using  @ # $ % & { [
		  
		  // Show our variables in bold
		  RTEdit1.VariablesTextBold = True
		  
		  // Let's add variables for demonstration
		  
		  // Let's use @ for people
		  RTEdit1.VariablesAt = Array("Garry", "John", "Lloyd", "Adam", "Steve", "Harold", "Peter", "David", "Ricky")
		  
		  // Let's use # for topics
		  RTEdit1.VariablesHash = Array("Xojo","Javascript","Java","Flutter","Dart","HTML","SQL","XCode")
		  
		  // Let's use [ for citations
		  RTEdit1.VariablesBrackets = Array("Full name", "Title", "Profession", "Workplace", "Phone")
		  
		  // Add Codes/Identifiers using $
		  RTEdit1.VariablesDollar = Array("ICD10:A00", "ICD10:B24", "CASE001", "CASE002", "SNOMED:123456")
		  
		  // Add template placeholders using {
		  RTEdit1.VariablesBraces = Array("Patient name", "Date of birth", "Current date", "Diagnosis", "Physician")
		  
		  // Add formulas, using %
		  RTEdit1.VariablesPercent = Array("BMI", "HbA1c", "Blood Pressure", "Cholesterol", "BodyFat")
		  
		  // Enable contextual clicks
		  RTEdit1.EnableRightClick = True
		  
		  
		  
		  
		  
		End Sub
	#tag EndEvent
	#tag Event
		Sub MouseclickRight(Method As String, Parameters() As Variant)
		  // This event is raised if RTEdit.EnableRightClick = True
		  
		  var m as DesktopMenuItem
		  Var popMenu As new DesktopMenuItem
		  
		  // Only show a popupmenu if some @ variables are defined
		  if me.VariablesAt.LastIndex = -1 then exit
		  
		  for atVariablesCount as integer = 0 to me.VariablesAt.LastIndex
		    m = new DesktopMenuItem(me.VariablesAt(atVariablesCount))
		    popMenu.AddMenu(m)
		    
		  next 
		  
		  var mSelected as DesktopMenuItem
		  mSelected =  popMenu.PopUp(system.MouseX, system.MouseY)
		  
		  // Insert the selected variable
		  if mSelected <> nil then
		    RTEdit1.InsertVariable(rtedit.VariableTypes.At,mSelected.Text.Trim)
		    
		  end if
		End Sub
	#tag EndEvent
#tag EndEvents
