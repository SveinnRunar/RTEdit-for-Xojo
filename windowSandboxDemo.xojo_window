#tag WindowCode
	#tag Event
		Sub Opening()
		  // customize the control (fired before EditorReady event)
		  self.Title = "RTEdit " + REdit.Version + " - Sandbox"
		  
		  DisplayThemeColors
		  
		  
		  
		  
		  
		End Sub
	#tag EndEvent


	#tag MenuHandler
		Function FileQuit() As Boolean Handles FileQuit.Action
		  Quit
		End Function
	#tag EndMenuHandler

	#tag MenuHandler
		Function mnuAboutRTEdit() As Boolean Handles mnuAboutRTEdit.Action
		  messagebox "RTEdit v" + RTEdit.Version
		  Return True
		  
		End Function
	#tag EndMenuHandler


	#tag Method, Flags = &h0
		Sub ApplyProperties()
		  // Reset color scheme
		  dropdownTheme.SelectedRowIndex = 0
		  DisplayThemeColors
		  
		  // Toolbar features (note, the clipboard will behave and allow paste of content, depending on which features are enabled)
		  REdit.AllowBold = checkBold.Value
		  REdit.AllowBulletins = checkBulletin.Value
		  REdit.AllowCleanFormatting = checkClean.Value
		  REdit.AllowCode = checkCode.Value
		  REdit.AllowFontSelection = checkFont.Value
		  REdit.AllowHeaders = checkHeadings.Value
		  REdit.AllowImage = checkMedia.value
		  REdit.AllowIndentation = checkIndentation.value
		  REdit.AllowItalic = checkItalic.Value
		  REdit.AllowLinks = checkLinks.Value
		  REdit.AllowList = checkList.value
		  REdit.AllowQuote = checkQuote.Value
		  REdit.AllowStrikeThrough = checkStrike.Value
		  REdit.AllowSubscript = checkSubscript.Value
		  REdit.AllowSuperscript = checkSuperscript.Value
		  REdit.AllowTextColor = checkTextColor.Value
		  REdit.AllowTextBackgroundColor = checkTextBackgroundColor.Value
		  REdit.AllowUnderline = checkUnderline.Value
		  REdit.AllowVariables = checkVariables.Value
		  REdit.AllowVideo = checkMedia.Value
		  
		  
		  // Modules
		  REdit.AllowTables = checkTable.Value
