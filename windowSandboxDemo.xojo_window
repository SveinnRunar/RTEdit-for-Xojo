		Private Sub ShowColorContextualMenu(base As DesktopMenuItem)
		  
		  Var m As DesktopMenuItem
		  
		  
		  // Copy value
		  m = New DesktopMenuItem("Copy color value")
		  m.Tag = "Copy"
		  base.AddMenu(m)
		  
		  // Paste value
		  m = new DesktopMenuItem("Paste color value")
		  m.Tag = "Paste"
		  if ClipboardColor.ToString.Trim <> "" then m.Enabled = true else m.Enabled = false
		  base.AddMenu(m)
		  
		End Sub
