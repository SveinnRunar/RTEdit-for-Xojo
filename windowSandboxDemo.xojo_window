	#tag Method, Flags = &h0
		Sub Initialize()
		  
		  // Initialize Rich Text control, called from REdit.Opening event (processed before EditorReady event
		  // -------------------------------------------------------------------------------------------------
		  
		  // Note, almost all of these properties can be defined with no code from the IDE.
		  
		  // Apply basic theme
		  if segmentbuttonThemes.SelectedSegmentIndex = 0 then REdit.Theme = RTEdit.Themes.Bubble else REdit.Theme = RTEdit.Themes.Snow
		  
		  // Apply properties, settings, colors etc. in the opening event (self.opening fires AFTER)
		  if dropdownTheme.SelectedRowIndex = 0 then 
		    REdit.ColorScheme = RTEdit.ColorSchemes.None
		    
		  elseif dropdownTheme.SelectedRowIndex = 1 then 
		    REdit.ColorScheme = RTEdit.ColorSchemes.Vanilla
		    
		  elseif dropdownTheme.SelectedRowIndex = 2 then
		    REdit.ColorScheme = RTEdit.ColorSchemes.Blueberry
		    
		  elseif dropdownTheme.SelectedRowIndex = 3 then
		    REdit.ColorScheme = RTEdit.ColorSchemes.Banana
		    
		  elseif dropdownTheme.SelectedRowIndex = 4 then
		    REdit.ColorScheme = RTEdit.ColorSchemes.Lime
		    
		  end if
		  
		  // Behaviour
		  REdit.EditMode = checkEditMode.Value // Do we start in edit mode or read-only mode
		  REdit.EnableRightClick = checkEnableRightClick.Value // Enable custom contextual clicks event
		  
		  // Let's define a placeholder
		  REdit.PlaceHolder = "Start typing here to create a new document..."
		  
		  // The following settings only apply to theme 'snow'
		  SetSnowProperties
		  
		  // Define toolbar buttons, Default are : bold, italic, underline, headings, bulletins, links
		  REdit.AllowAlignment = False
		  REdit.AllowBold = checkBold.Value
		  REdit.AllowBulletins = checkBulletin.Value
		  REdit.AllowCode = checkCode.Value
		  REdit.AllowFontSelection = checkFont.Value
		  REdit.AllowHeaders = checkHeadings.Value
		  REdit.AllowIndentation = checkIndentation.Value
		  REdit.AllowItalic = checkItalic.Value
		  REdit.AllowImage = checkMedia.Value
		  REdit.AllowLinks = checkLinks.Value
		  REdit.AllowList = checkList.Value
		  REdit.AllowQuote = checkQuote.value
		  REdit.AllowStrikeThrough = checkStrike.Value
		  REdit.AllowSubscript = checkSubscript.Value
		  REdit.AllowSuperscript = checkSuperscript.Value
		  REdit.AllowTextBackgroundColor = checkTextBackgroundColor.Value
		  REdit.AllowUnderline = checkUnderline.Value
		  REdit.AllowTextColor = checkTextColor.Value
		  
		  // Modules
		  REdit.AutoFormatLinks = checkAutoFormatLinks.Value // Automatically convert e-mails and URL's to clickable elements when typing. True by default
		  REdit.AllowTextDragDrop = checkTextDragdrop.Value // Allow text to be dragged & dropped into the control, either from within or outside. True by default
		  REdit.KeyboardBindStyles = checkKeyboardBinding.Value // Keyboard Binding Styles is true by default, it enables Command+1/2/3/4 for styling
		  REdit.AllowTables = checkTable.Value // By default, tables are allowed, only enabled when using theme 'Snow'
		  REdit.AllowTextDragDrop = checkTextDragdrop.Value // Allow text to be draggd into the editor
		  REdit.AllowVariables = checkVariables.Value
		  
		  // Toolbar, note that Bubble theme has a floating toolbar, appearing on content selection.
		  if segmentbuttonThemes.SelectedSegmentIndex = 0 then
		    EnableSnowProperties(False)
		    
		    // Snow has toolbar
		  else
		    // Default alignment is left
		    if checkAlignToolbarCenter.Value = True then REdit.ToolbarAlignment = RTEdit.ToolBarAlignments.Center
		    
		  end if
		  
		  
		  // Variables
		  // ----------
		  
		  // Enable Variables
		  REdit.AllowVariables = True
		  
		  // Style variable text
		  REdit.VariablesTextBold = true
		  
		  // We want to allow variables, let's feed in some bracket variables
		  REdit.VariablesBrackets = Array("Patient", "SSID", "Address", "Phone", "Gender","Age", "Birthyear")
		  
		  // And some brace variables
		  REdit.VariablesBraces = Array("Full name", "Title", "Profession", "Workplace", "Phone")
		  
		  // Add some % variables
		  REdit.VariablesPercent = Array("TSH","T4","B12","D-Vitamin","ESR","LEU","Krea","Na","K","Ca","P","Crp")
		  
		  // Add some # variables
		  REdit.VariablesHash = Array("General advice", "Paracetamol 1g for fever", "Visit again if symptoms worsen","Doxylin 100mg, 2 tablets the first day, then one a day","Doxylin 1 tablet 2x a day for 2 weeks", "Azithromycin 500mg 1 tablet 1x a day for 3 days","Vitals every 4 hours","Vitals once per shift", "Vitals every 8 hours", "Blood culturing if fever > 38.5°")
		  
		  // Add some $ variables
		  REdit.VariablesDollar = Array("SaO2","T","P","Bp","Kg","RR")
		  
		  // Add some @ variables
		  REdit.VariablesAt = Array("Garry", "John", "Lloyd", "Adam", "Steve", "Harold", "Peter", "David", "Ricky")
		  
		  // Note, if you want to postbone the RTEdit.Initialize event and do it manually, you set:
		  // REdit.AutoInitialize = False
		  // This means that after setting your properties and settings, you need to fire the Initialize event manually:
		  // REdit.Initialize
		  
		  
		End Sub
	#tag EndMethod

	#tag Method, Flags = &h21
		Private Sub LoadDemoDocumentFromJSON()
		  REdit.ImportDocumentFromJSON(K_WHATS_NEW, true, true, true, true, true, true)
		  
		  var s as string = REdit.document
		  
		  blockControlUpdates = true
		  
		  // Show the scheme colors
		  DisplayThemeColors
		  
		  // Behaviour
		  checkEnableRightClick.Value = REdit.EnableRightClick
		  
		  // Modules
		  checkKeyboardBinding.Value = REdit.KeyboardBindStyles
		  checkTable.Value = REdit.AllowTables
		  checkAutoFormatLinks.Value = REdit.AutoFormatLinks
		  checkVariables.Value = REdit.AllowVariables
		  checkTextDragdrop.Value = REdit.AllowTextDragDrop
		  
		  // Load the properties into our demo window
		  checkBold.Value = REdit.AllowBold
		  checkBulletin.Value = REdit.AllowBulletins
		  checkClean.Value = REdit.AllowCleanFormatting
		  checkCode.Value = REdit.AllowCode
		  checkFont.Value = REdit.AllowFontSelection
		  checkHeadings.Value = REdit.AllowHeaders
		  checkMedia.Value = REdit.AllowImage
		  checkIndentation.Value = REdit.AllowIndentation
		  checkItalic.Value = REdit.AllowItalic
		  checkLinks.Value = REdit.AllowLinks
		  checkList.Value = REdit.AllowList
		  checkQuote.Value = REdit.AllowQuote
		  checkStrike.Value = REdit.AllowStrikeThrough
		  checkSubscript.Value = REdit.AllowSubscript
		  checkSuperscript.Value = REdit.AllowSuperscript
		  checkTextColor.Value = REdit.AllowTextColor
		  checkTextBackgroundColor.Value = REdit.AllowTextBackgroundColor
		  checkUnderline.Value = REdit.AllowUnderline
		  
		  // Toolbar
		  checkToolbar.Value = REdit.Toolbar
		  checkToolbarBorder.Value = REdit.ToolbarBorder
		  
		  
		  // State
		  checkEditMode.Value = REdit.EditMode
		  
		  blockControlUpdates = false
		  
		  //UpdateStyling(segmentbuttonThemes.SelectedSegmentIndex)
		  REdit.Document = s
		End Sub
