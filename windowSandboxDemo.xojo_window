#tag Events buttonGetDocumentVariables
	#tag Event
		Sub Pressed()
		  var variableArray() as string = REdit.getDocumentVariables
		  
		  if variableArray.LastIndex <> -1 then 
		    textFeedback.Text = ""
		    
		    var arrayLastIndex As Integer = variableArray.LastIndex
		    for i as integer = 0 to arrayLastIndex
		      textFeedback.Text = textFeedback.Text + variableArray(i) + EndOfLine
		      
		    next
		    
		  else
		    textFeedback.Text = "Document uses no variables. Use any variable denotion character (@ # $ % { [ ) to insert variables into your document or press 'Edit Variables' to edit and create new ones."
		    
		  end if
		  
		End Sub
	#tag EndEvent
#tag EndEvents
